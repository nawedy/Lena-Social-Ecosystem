# Test Environment Configuration

# Supabase Configuration
SUPABASE_URL=http://localhost:54321
SUPABASE_ANON_KEY=your-test-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-test-service-role-key

# Test User Credentials
TEST_USER_EMAIL=test@example.com
TEST_USER_PASSWORD=Password123!

# Test Database Configuration
TEST_DATABASE_URL=postgresql://postgres:postgres@localhost:54322/postgres

# Test Storage Configuration
TEST_STORAGE_BUCKET=test-bucket

# Test Payment Configuration (Stripe Test Keys)
STRIPE_PUBLIC_KEY=pk_test_your_test_key
STRIPE_SECRET_KEY=sk_test_your_test_key
STRIPE_WEBHOOK_SECRET=whsec_your_test_webhook_secret

# Test Email Configuration (Mailhog)
SMTP_HOST=localhost
SMTP_PORT=1025
SMTP_USER=test
SMTP_PASSWORD=test

# Test Redis Configuration
REDIS_URL=redis://localhost:6379/1

# Test API Keys
TEST_API_KEY=test_api_key_123

# Test Feature Flags
ENABLE_TEST_FEATURES=true
ENABLE_TEST_ANALYTICS=true
ENABLE_TEST_NOTIFICATIONS=true

# Test Endpoints
API_ENDPOINT=http://localhost:3000/api
WEBSOCKET_ENDPOINT=ws://localhost:3000/ws

# Test Media Storage
TEST_MEDIA_PATH=/tmp/test-media

# Test Logging
LOG_LEVEL=debug
ENABLE_TEST_LOGGING=true

# Test Performance Thresholds
MAX_RESPONSE_TIME_MS=1000
MAX_MEMORY_USAGE_MB=512

# Test Rate Limiting
TEST_RATE_LIMIT_REQUESTS=100
TEST_RATE_LIMIT_WINDOW_MS=60000

# Test Security
ENABLE_TEST_CSRF=true
ENABLE_TEST_CORS=true
TEST_JWT_SECRET=your_test_jwt_secret

# Test Monitoring
ENABLE_TEST_MONITORING=true
TEST_SENTRY_DSN=https://test@sentry.io/123

# Test Cache Configuration
TEST_CACHE_TTL=300
ENABLE_TEST_CACHE=true

# Test Search Configuration
TEST_SEARCH_INDEX=test_index
ENABLE_TEST_SEARCH=true

# Test Blockchain Configuration
TEST_NETWORK_URL=http://localhost:8545
TEST_PRIVATE_KEY=0x0123456789abcdef0123456789abcdef0123456789abcdef0123456789abcdef

# Test AI/ML Configuration
TEST_MODEL_PATH=/tmp/test-models
ENABLE_TEST_ML=true

# Test Metrics Collection
ENABLE_TEST_METRICS=true
TEST_METRICS_PORT=9090

# Test Feature Toggles
ENABLE_EXPERIMENTAL_FEATURES=false
ENABLE_BETA_FEATURES=true

# Test Compliance
ENABLE_TEST_AUDIT_LOGS=true
TEST_COMPLIANCE_MODE=strict

# Test Internationalization
DEFAULT_TEST_LOCALE=en
AVAILABLE_TEST_LOCALES=en,es,fr

# Test Dependencies
ENABLE_EXTERNAL_SERVICES=false
MOCK_EXTERNAL_APIS=true

# Test Browser Configuration
HEADLESS_MODE=true
SCREENSHOT_PATH=/tmp/test-screenshots
VIDEO_PATH=/tmp/test-videos

# Test Report Configuration
TEST_REPORT_PATH=/tmp/test-reports
ENABLE_HTML_REPORTS=true
ENABLE_JSON_REPORTS=true

# Test Cleanup Configuration
ENABLE_AUTO_CLEANUP=true
CLEANUP_OLDER_THAN_DAYS=7

# Test Performance Monitoring
ENABLE_PERFORMANCE_MONITORING=true
COLLECT_COVERAGE_DATA=true

# Test Error Handling
ENABLE_ERROR_TRACKING=true
MAX_RETRY_ATTEMPTS=3

# Test Data Generation
USE_FAKE_DATA=true
SEED_DATABASE=true

# Test Timeouts
DEFAULT_TIMEOUT_MS=5000
LONG_RUNNING_TEST_TIMEOUT_MS=30000

# Test Parallelization
MAX_PARALLEL_TESTS=4
ENABLE_TEST_SHARDING=true 